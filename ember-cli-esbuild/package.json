{
  "name": "ember-cli-esbuild",
  "version": "1.0.0",
  "description": "Ember addon for transpiling with ESBuild",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
